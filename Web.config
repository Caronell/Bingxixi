<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.0"/>
    <httpRuntime executionTimeout="240" maxRequestLength="8192000"/>
    <sessionState mode="InProc" cookieless="false" timeout="9000"/>
  </system.web>
  <system.webServer>
    <directoryBrowse enabled="true"/>
  </system.webServer>
  <appSettings>
    <add key="constr" value="database = dbname; Password = yourpwd; User ID = yourid; server = yourserver"/>
  </appSettings>
</configuration>
